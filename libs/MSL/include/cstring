#ifndef MSL_CSTRING_H
#define MSL_CSTRING_H

#include <string.h>

#ifdef __cplusplus

namespace std {
    using   ::memcpy;
    using   ::memset;
    using   ::memmove;

    using   ::memcmp;

    using   ::strcpy;
    using   ::strcmp;

    using   ::stricmp;
    
    using   ::strcat;

    using   ::strncpy;
    using   ::strncat;
    using   ::strncmp;

    using   ::strlen;
    using   ::strnlen;
} // namespace std

#endif

#endif // MSL_CSTRING_H
